{
  "name": "AFMS",
  "description": "This app does one little thing, and does it well.",
  "version":"1.0.0",
  "engines": {
    "npm": "6.x"
  },
  "keywords": [
    "productivity",
    "HTML5",
    "scalpel"
  ],
  "website": "https://AFMS.com/",
  "repository": "https://github.com/sriy333/AFMS",
  "logo": "https://small-sharp-tool.com/logo.svg",
  "success_url": "/welcome",
  "scripts": {
    "postdeploy": "bundle exec rake bootstrap"
  },
